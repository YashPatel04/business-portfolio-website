{
    "version": 2,
    "builds": [
      {
        "src": "/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "/api/email.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/email.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  